{
  "name": "litera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "npm-run-all --serial lint",
    "lint": "npm-run-all --serial lint:scripts lint:text",
    "lint:commit": "yarn commitlint --verbose --color --from $(git merge-base origin/main HEAD)",
    "lint:scripts": "next lint",
    "lint:text": "textlint ./",
    "lint:text:fix": "yarn lint:text --fix",
    "format": "yarn format:check",
    "format:check": "prettier --check ./",
    "format:fix": "prettier --write ./"
  },
  "dependencies": {
    "@types/node": "18.19.62",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "eslint": "8.57.1",
    "eslint-config-next": "13.5.7",
    "next": "14.2.16",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "^1.57.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@lmc-eu/prettier-config": "^2.0.1",
    "@lmc-eu/textlint-rule-preset-lmc": "^2.0.1",
    "commitlint": "^19.5.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "textlint": "^14.3.0",
    "vercel": "^37.7.1"
  },
  "packageManager": "yarn@4.5.1"
}
